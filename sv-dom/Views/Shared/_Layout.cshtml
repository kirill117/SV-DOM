    @Scripts.Render("~/bundles/modernizr")


    <script>
        $(document).ready(function () {
            $(".image_box, .overlay_image_box").click(function () {
                var this_section = $(this).closest('.section');
                if (this_section.hasClass('img_zoom')) {
                    var image_url = $(this).parent().children().attr('src');
                    var t_arr = image_url.split('/');
                    if ($('#img_zoomer_bg').size()) {
                        $('#img_zoomer_bg').remove();
                    }
                    $('body').append('<div id="img_zoomer_bg"><div id="img_zoomer" style="background:url(' + image_url + ') no-repeat center; background-size:contain;"></div><div id="img_zoomer_close">X</div></div>');
                    var p = $('#img_zoomer_close').parent().position().left + $('#img_zoomer_close').parent().width() - 100;
                    $('#img_zoomer_close').css('left', p);
                }
            });

            $(".btn_recall").on('click', function (e) {
                e.preventDefault();
                $('.popup_form').show();
            });

            $(".popup_form_close").on('click', function () {
                $('.popup_form').hide();
            });

            $('body').on('click', '#img_zoomer_bg ,#img_zoomer_close', function () {
                if ($('#img_zoomer_bg').size()) {
                    $('#img_zoomer_bg').remove();
                }

            })

            $('body').on('click', '.proj-cart', function () {
                var id = $(this).attr('item');
                if (id && id.length > 0) {
                    window.location.href = "project/" + id;
                }
            });

            $('.form-get-recall, .form-get-info').on('click', '.submit_btn', function () {
                $('.popup_form').hide();
                var frm = $(this).parent().parent().parent();
                $.ajax({
                    type: 'POST',
                    url: '/GetRecall',
                    data:
                    {
                        name: frm.find('.input-name').val(),
                        phone: frm.find('.input-phone').val(),
                        email: frm.find('.input-mail').val()
                },
                    success: function (content) {
                    },
                    traditional: true,
                    dataType: 'json'
                });

                    frm.find('input[type="text"], input[type="email"]').each(function () {
                    $(this).val('');
                });

                $('.popup_thanks').show();
            });

            $(".popup_thanks_close").on('click', function () {
                $('.popup_thanks').hide();
            });
        });
    </script>

</head>

